        "import os\n",
        "\n",
        "# JSON 파일이 저장된 디렉터리 경로 설정 (예: 'MyDrive/json_files/')\n",
        "json_dir = '/content/drive/MyDrive/Colab Notebooks/sequencing_performance'\n",
        "\n",
        "def load_json_files(directory):\n",
        "    json_files = [f for f in os.listdir(directory) if f.endswith('.json')]\n",
        "    json_files.sort()  # 파일명 정렬\n",
        "    return json_files\n",
        "\n",
        "def merge_json_files(directory, json_files):\n",
        "    merged_data = []\n",
        "\n",
        "    # 모든 JSON 파일을 병합\n",
        "    for file in json_files:\n",
        "        with open(os.path.join(directory, file), 'r') as f:\n",
        "            data = json.load(f)\n",
        "            merged_data.append(data)\n",
        "\n",
        "    return merged_data\n",
        "\n",
        "def extract_first_and_last(json_files):\n",
        "    # 시작과 마지막 JSON 파일 추출\n",
        "    return json_files[0], json_files[-1]\n",
        "\n",
        "# JSON 파일 목록 불러오기\n",
        "json_files = load_json_files(json_dir)\n",
        "\n",
        "# 병합된 데이터와 시작/마지막 파일 추출\n",
        "merged_data = merge_json_files(json_dir, json_files)\n",
        "first_json, last_json = extract_first_and_last(json_files)\n",
        "\n",
        "print(f\"First JSON: {first_json}\")\n",
        "print(f\"Last JSON: {last_json}\")\n",
        "print(f\"Merged JSON Data: {merged_data}\")\n"
